<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ColorFilterImageView">
        <attr name="colorFilter" format="color"/>
    </declare-styleable>

    <declare-styleable name="NameView">
        <attr name="nv_primaryTextColor" format="color"/>
        <attr name="nv_secondaryTextColor" format="color"/>
        <attr name="nv_twoLine" format="boolean"/>
        <attr name="nv_primaryTextStyle"/>
        <attr name="nv_secondaryTextStyle"/>
        <attr name="nv_primaryTextSize" format="dimension"/>
        <attr name="nv_secondaryTextSize" format="dimension"/>
        <attr name="nv_secondaryTextScale" format="fraction"/>
    </declare-styleable>
    <attr name="nv_primaryTextStyle">
        <flag name="normal" value="0"/>
        <flag name="bold" value="1"/>
        <flag name="italic" value="2"/>
    </attr>
    <attr name="nv_secondaryTextStyle">
        <flag name="normal" value="0"/>
        <flag name="bold" value="1"/>
        <flag name="italic" value="2"/>
    </attr>
</resources>